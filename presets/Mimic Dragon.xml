<l-system xmlns="http://chubarov.net/Lindenmayer" version="1.0">
        
    <rules axiom="FX">
        <rule symbol="X">X+YF+</rule>
        <rule symbol="Y">-FX-Y</rule>
    </rules>
    <options>
    	<option name="width">1024</option>
    	<option name="height">800</option>
    	<option name="background">#008800</option>
        <option name="defaultOrder">17</option>
        <option name="turtle.turnAngle">84</option>
        <option name="turtle.growFactor">0.9</option>
    </options>
</l-system>