<l-system xmlns="http://chubarov.net/Lindenmayer" version="1.0">
    <rules axiom="VZFFF">
        <rule symbol="V" mean="F">[+++W][---W]YV</rule>
        <rule symbol="W" mean="F">+X[-W]Z</rule>
        <rule symbol="X" mean="F">-W[+X]Z</rule>
        <rule symbol="Y" mean="F">YZ</rule>
        <rule symbol="Z" mean="F">[-FFF][+FFF]Z</rule>
    </rules>
    <options>
        <option name="defaultOrder">10</option>
        <option name="turtle.initialAngle">270</option>
        <option name="turtle.turnAngle">20</option>
    </options>
</l-system>