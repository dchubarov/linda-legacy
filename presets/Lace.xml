<l-system xmlns="http://chubarov.net/Lindenmayer" version="1.0">
    <description>Lace (d. chubarov)</description>
    <rules axiom="X">
        <rule symbol="X" mean="F">X++Y++X</rule>
        <rule symbol="Y" mean="F">Y-Y</rule>
    </rules>
    <options>
        <option name="defaultOrder">11</option>
        <option name="turtle.turnAngle">21</option>
        <option name="foreground">#efefef</option>
    </options>
</l-system>