<l-system xmlns="http://chubarov.net/Lindenmayer" version="1.0">
    <description>Sierpinski Triangle</description>
    <options>
        <option name="defaultOrder">6</option>
        <option name="turtle.turnAngle">60</option>
        <option name="turtle.growFactor">1</option>
    </options>
    <rules axiom="A">
        <rule symbol="A" mean="F">B-A-B</rule>
        <rule symbol="B" mean="F">A+B+A</rule>
    </rules>
</l-system>
